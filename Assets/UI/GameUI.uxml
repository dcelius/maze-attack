            </ui:VisualElement>
            <ui:VisualElement name="IconPanel" style="flex-grow: 0.125; align-items: center; width: 100%;">
                <ui:VisualElement name="IconImage" style="flex-grow: 1; margin-top: 10px; margin-bottom: 5px; background-image: url(&apos;project://database/Assets/Clean%20Vector%20Icons/Update/bonus_04.png?fileID=21300000&amp;guid=ef147e2665db3394ab8f3c2ade98cbf5&amp;type=3#bonus_04&apos;); width: 60%; height: 60%; min-width: 60px; min-height: 60px;" />
                <ui:Label text="R - Switch" display-tooltip-when-elided="true" style="width: auto; font-size: 24%;" />
