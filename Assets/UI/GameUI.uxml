            <ui:VisualElement name="PauseIconPanel" style="flex-grow: 0.125; align-items: center; width: 100%;">
                <ui:VisualElement name="IconImage" style="flex-grow: 1; padding-top: 0; background-image: url(&apos;project://database/Assets/Clean%20Vector%20Icons/T_8_pause_.png?fileID=21300000&amp;guid=a1bda28f797f71c4db09f6a0d4af4759&amp;type=3#T_8_pause_&apos;); height: 60%; width: 60%; margin-bottom: 5px; min-width: 60px; min-height: 60px; margin-top: 10px;" />
                <ui:Label text="Esc - Pause" display-tooltip-when-elided="true" style="width: auto; font-size: 24%; background-image: none;" />
