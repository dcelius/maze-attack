            <ui:VisualElement name="IconPanel" style="flex-grow: 0.125; align-items: center; width: 100%;">
                <ui:VisualElement name="IconImage" style="flex-grow: 1; margin-top: 10px; margin-bottom: 5px; background-image: url(&apos;project://database/Assets/Clean%20Vector%20Icons/T_10_hammer_.png?fileID=21300000&amp;guid=7dfd68325eeac354d866d207e4d2744c&amp;type=3#T_10_hammer_&apos;); width: 60%; height: 60%; min-width: 60px; min-height: 60px; align-items: auto;" />
